{
    "name": "antimattr/etl",
    "type": "library",
    "description": "ETL processing for extracting, transforming, and loading data",
    "keywords": ["database", "dependency injection", "etl", "extract", "load", "mongodb", "mysql", "transform", "warehouse"],
    "homepage": "http://github.com/antimattr/etl",
    "license": "MIT",
    "authors": [
        {"name": "Matthew Fitzgerald", "email": "matthewfitz@gmail.com"}
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/collections": ">=1.0",
        "psr/log": ">=1.0",
        "symfony/dependency-injection": ">=2.6",
        "symfony/config": ">=2.6",
        "symfony/console": ">=2.6"
    },
    "require-dev": {
        "antimattr/test-case": ">=1.0.1",
        "phpunit/phpunit": ">=4.0",
        "fabpot/php-cs-fixer": ">=v0.5.7",
        "monolog/monolog": ">=1.8"
    },
    "suggest": {
        "monolog/monolog": "Sends your logs to files, sockets, inboxes, databases and various web services"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "AntiMattr\\ETL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "AntiMattr\\TestCase\\": "vendor/antimattr/test-case/src",
            "AntiMattr\\Tests\\ETL\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "archive": {
        "exclude": ["bin", "tests", "*phpunit.xml"]
    }
}
